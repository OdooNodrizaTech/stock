<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_template_property_form_arelux_stock_picking_delivery_carrier" model="ir.ui.view">
        <field name="name">view_template_property_form_arelux_stock_picking_delivery_carrier</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock.view_template_property_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page//field[@name='tracking']" position="before">
                <field name="carrier_id"/>
            </xpath>
        </field>
    </record>

</odoo>