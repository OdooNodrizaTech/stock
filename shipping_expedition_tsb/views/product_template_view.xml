<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="shipping_expedition_tsb_view_template_property_form" model="ir.ui.view">
        <field name="name">shipping_expedition_tsb_view_template_property_form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock.view_template_property_form"/>
        <field name="arch" type="xml">      
            <xpath expr="//page//field[@name='tracking']" position="before">
                <field name="tsb_sender_center"/>
            </xpath>      
        </field>
    </record>

</odoo>
