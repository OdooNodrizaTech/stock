<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_shipping_expedition_tsb_form" model="ir.ui.view">
        <field name="model">shipping.expedition</field>
        <field name="inherit_id" ref="base_delivery_carrier_expedition.shipping_expedition_form_view"/>
        <field name="arch" type="xml">
            <field name="exps_rels" position="after">   
                <field name="tsb_identiticket" readonly="1" attrs="{'invisible': [('carrier_type','!=', 'tsb')]}"/>
                <field name="tsb_localizator" readonly="1" attrs="{'invisible': [('carrier_type','!=', 'tsb')]}"/>
                <field name="tsb_url" widget="url" attrs="{'invisible': [('carrier_type','!=', 'tsb')]}"/>
            </field>
        </field>
    </record>   
</odoo>