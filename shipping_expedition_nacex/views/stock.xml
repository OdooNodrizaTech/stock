<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_picking_withcarrier_out_form" model="ir.ui.view">
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form"/>
        <field name="arch" type="xml">
        <field name="state" position="before">
            <button name="action_view_etiqueta"
                attrs="{'invisible': [('shipping_expedition_id', '==', '0')]}"
                string="Ver etiqueta" type="object"/>      
        </field>
        </field>
  </record>
  <record id="view_picking_internal_search_delivery_carrier_expedition_nacex" model="ir.ui.view">
        <field name="name">view_picking_internal_search_delivery_carrier_expedition_nacex</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_internal_search" />
        <field name="priority" eval="22"/>
        <field name="arch" type="xml">
            <filter name="done" position="before">
                <filter string="NACEX" name="nacex_filter" domain="[('carrier_id.carrier_type','=','nacex')]"/>                
                <!--<separator/>!-->                
            </filter>                                                                                                                
        </field>
    </record>
</odoo>